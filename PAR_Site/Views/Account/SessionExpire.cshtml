@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>SessionExpire</title>
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Scripts/bootstrap.min.js"></script>
</head>
<body>
    @using (Html.BeginForm())
    {
        <div class="container">
            <div class="col-lg-12" style="padding-top:30px;">
                <div class="col-lg-9">
                    <img src="~/Images/UDCLogo.png" />
                    <div class="col-lg-12" style="background-color:lightskyblue;margin-top:5px;color:white;">
                        <h5><b>Session Expire</b></h5>
                    </div>
                    <br />
                    <br />
                    <br />
                    <br />
                    <div class="alert alert-warning">
                        <strong>Warning!</strong> Session is expired. Please Login Again.
                    </div>
                    <div class="form-group">
                        @Html.ActionLink("Back To Login", "Login", null, new { @class = "btn btn-primary" })
                    </div>
                </div>
            </div>
            <div class="">
                
            </div>
        </div>

    }
</body>
</html>